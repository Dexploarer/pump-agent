{
  "name": "pump-agent",
  "version": "1.0.0",
  "type": "module",
  "description": "Real-time cryptocurrency data collection and AI query platform for pump.fun tokens",
  "main": "dist/main.js",
  "scripts": {
    "build": "bun build src/main.ts --outdir dist --target node --external better-sqlite3",
    "build:dev": "bun build src/main-dev.ts --outdir dist --target node --external better-sqlite3",
    "start": "node dist/main.js",
    "start:prod": "node dist/main.js",
    "dev": "bun run src/main-dev.ts",
    "test": "bun test",
    "test:unit": "bun test tests/unit/",
    "test:integration": "bun test tests/integration/",
    "test:e2e": "bun test tests/e2e/",
    "clean": "rm -rf dist/ data/ logs/",
    "lint": "eslint src/ --ext .ts",
    "lint:fix": "eslint src/ --ext .ts --fix",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "cryptocurrency",
    "pump.fun",
    "mcp",
    "influxdb",
    "websocket"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@influxdata/influxdb-client-apis": "^1.33.2",
    "@modelcontextprotocol/sdk": "^0.4.0",
    "better-sqlite3": "^12.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "open": "^10.2.0",
    "socket.io": "^4.7.2",
    "winston": "^3.17.0",
    "ws": "^8.14.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.14.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.4.0"
  }
}
